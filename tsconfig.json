{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@gStyle": [
        "./styles.ts"
      ],
      "@hooks": [
        "hooks/*"
      ],
      "@assets": [
        "./assets/*"
      ]
    }
  }
}